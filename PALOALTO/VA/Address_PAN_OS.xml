  <example>
    <content>&lt;response status=&quot;success&quot;&gt;
    &lt;result&gt;
        &lt;address&gt;
            &lt;entry name=&quot;testAddress&quot;&gt;
                &lt;ip-netmask&gt;1.1.1.1/30&lt;/ip-netmask&gt;
            &lt;/entry&gt;
            &lt;entry name=&quot;sdi_test&quot;&gt;
                &lt;ip-netmask&gt;1.1.1.1/32&lt;/ip-netmask&gt;
                &lt;description&gt;sdi test&lt;/description&gt;
            &lt;/entry&gt;
            &lt;entry name=&quot;google&quot;&gt;
                &lt;fqdn&gt;google.com&lt;/fqdn&gt;
                &lt;description&gt;google&lt;/description&gt;
            &lt;/entry&gt;
            &lt;entry name=&quot;plage address&quot;&gt;
                &lt;ip-range&gt;10.0.0.1-10.0.0.4&lt;/ip-range&gt;
                &lt;description&gt;Saisissez une plage d&apos;adresses IP (par exemple, 10.0.0.1-10.0.0.4). Chaque adresse IP peut apparaître au format IPv6 (par exemple, 2001:db8:123:1::1-2001:db8:123:1::11)&lt;/description&gt;
            &lt;/entry&gt;
            &lt;entry name=&quot;masque_de_caractere_generique&quot;&gt;
                &lt;ip-wildcard&gt;10.20.1.0/0.0.248.255&lt;/ip-wildcard&gt;
                &lt;description&gt; un masque de caractère générique IP (par exemple, 10.20.1.0/0.0.248.255).&lt;/description&gt;
            &lt;/entry&gt;
            &lt;entry name=&quot;gmail&quot;&gt;
                &lt;fqdn&gt;gmail.com&lt;/fqdn&gt;
            &lt;/entry&gt;
            &lt;entry name=&quot;2a03_eec0_3221___slash_48&quot;&gt;
                &lt;ip-netmask&gt;2a03:eec0:3221::/48&lt;/ip-netmask&gt;
            &lt;/entry&gt;
        &lt;/address&gt;
    &lt;/result&gt;
&lt;/response&gt;</content>
  </example>
  <command name="IMPORT">
    <operation><![CDATA[type=config&action=show]]></operation>
    <post_template>{if $params.tag == &quot;fqdn&quot;}
    {assign_object_variable var=&quot;type&quot; value=&quot;fqdn&quot;}
{else $params.tag == &quot;ip-netmask&quot;}
	{assign var=&quot;is_ipv6&quot; value=$params.ip|strpos:&quot;:&quot;}
	{if $is_ipv6 == true}
    	{assign_object_variable var=&quot;type&quot; value=&quot;cidr_ipv6&quot;}
	{else}
		{assign_object_variable var=&quot;type&quot; value=&quot;cidr_ipv4&quot;}
	{/if}
{/if}</post_template>
    <parser>
      <section>
        <regexp>@^\s \s*$@</regexp>
        <xpath>//entry</xpath>
      </section>
      <lines>
        <line>
          <regexp>@name=&quot;(?&lt;object_id&gt;.*)&quot;@</regexp>
          <xpath>//entry/@name</xpath>
        </line>
        <line>
          <regexp>@&lt;address&gt;(?&lt;ip_wildcard&gt;.*)&lt;/address&gt;@</regexp>
          <xpath></xpath>
        </line>
        <line>
          <regexp>@&lt;fqdn&gt;(?&lt;ip&gt;.*)&lt;/fqdn&gt;@</regexp>
          <xpath>//fqdn</xpath>
        </line>
        <line>
          <regexp>@&lt;entry name=&quot;[^&quot;]*&quot;&gt;\s*&lt;(?&lt;tag&gt;\w[\w\-]*)&gt;@</regexp>
          <xpath>//entry</xpath>
        </line>
        <line>
          <regexp>@&lt;ip-netmask&gt;(?&lt;ip&gt;.*)/(?&lt;prefix&gt;.*)&lt;/ip-netmask&gt;@</regexp>
          <xpath>//entry</xpath>
        </line>
      </lines>
    </parser>
    <xpath>/config/devices/entry[@name=&apos;localhost.localdomain&apos;]/vsys/entry[@name=&apos;vsys1&apos;]/address</xpath>
  </command>
  <command name="CREATE">
    <operation><![CDATA[<entry name="{$params.object_id}">
	{if $params.type == "cidr_ipv4" || $params.type == "cidr_ipv6"}
		<ip-netmask>{$params.ip}/{$params.prefix}</ip-netmask>
	{/if}
	{if $params.type == "fqdn"}
		<fqdn>{$params.ip}</fqdn>
	{/if}
</entry>]]></operation>
    <rest>type=config&amp;action=set</rest>
    <xpath>/config/devices/entry[@name=&apos;localhost.localdomain&apos;]/vsys/entry[@name=&apos;vsys1&apos;]/address</xpath>
  </command>
